package Sharonjava;
import java.util.Scanner;
public class odd
{
	public static void main(String ar[])
	{
		Scanner s=new Scanner(System.in);
		System.out.print("Enter a number: ");
		int num=s.nextInt();
		if(num>0)
		{
			System.out.print("positive");
		}
		else
		{
			System.out.print("negative");
		}
	}
}