package Sharonjava;
import java.util.Scanner;

public class odd
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter a number : ");
		int a=s.nextInt();
		if(a>0)
		{
			System.out.println("positive");
		}
		else
		{
			System.out.println("negative");
		}
	}
}
